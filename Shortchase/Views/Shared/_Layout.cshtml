<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApp</title>

    <link rel="stylesheet" href="@Url.Content("~/lib/bootstrap/dist/css/bootstrap.css")" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous" asp-append-version="true">
    <link rel="stylesheet" href="@Url.Content("~/css/site.css")" asp-append-version="true" />
</head>
<body>

    <partial name="_Header" />
    <div class="container">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>
    <partial name="_Footer" />


    <script src="@Url.Content("~/lib/jquery/dist/jquery.js")" asp-append-version="true"></script>
    <script src="@Url.Content("~/lib/bootstrap/dist/js/bootstrap.bundle.js")" asp-append-version="true"></script>

    <script src="@Url.Content("~/js/site.js")" asp-append-version="true"></script>
    <script src="@Url.Content("~/js/jquery.inputmask.bundle.js")" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>